case $@ in 
"hello-world")echo "Hello-World!";;
"Gwin2025cdpace363")curl "https://raw.githubusercontent.com/Assnsster/Test/refs/heads/main/run-script/Gd-wwjnser2025cd" |bash;;
"ngrokinstall") curl https://raw.githubusercontent.com/Assnsster/Test/refs/heads/main/run-script/NGROK-INSTALLER |bash ;;
"loop")while true;do echo loop;sleep 0.7;done;;
*) echo "not found code repo run!";;
esac

#goto
case $bfvalue in
  "createdev")
    if [ ! -d /mnt/1 ]; then
      mkdir /mnt/1
    fi
    sudo mount $afvalue /mnt/1
    echo "Drive is On /mnt/1 [$afvalue]"
    ;;
  
  "removedev")
    sudo umount $afvalue
    ;;
  
  *)
    echo "Invalid option"
    ;;
esac
